<% for (var o=octave; o>0; o--) { %>
<% _.each(pitches, function(pitch, i) { %>
<% var midiPitch = ((12*(o+1))+(pitches.length-(i+1))); %>
<div class="pitch <%= pitch %><%= o %> p-<%= midiPitch %>">
<% for (var n=1; n<=sub; n++) { %>
<% var cellId = block+"-"+sub+"-"+midiPitch+"-"+n; %>
<div id="<%= cellId %>" class="cell sc-<%= n %>"></div>
<% }; %>
</div>
<% }); %>
<% }; %>